(:name magit
       :website "https://github.com/ponkore/magit#readme"
       :description "It's Magit! An Emacs mode for Git."
       :type github
       :pkgname "ponkore/magit"
       :info "."
       :autoloads ("50magit")
       :build (("make" "all"))
       :build/darwin `(("make"
                        ,(format "EMACS=%s" el-get-emacs)
                        "all")))
